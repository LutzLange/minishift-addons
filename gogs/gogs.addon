# Name: gogs
# Description: Create gogs project.

oc new-project gogs
oc new-app -f gogs-persistent-template.yaml --param=HOSTNAME=gogs.#{ip}.nip.io --param=SKIP_TLS_VERIFY=true --param=GOGS_VERSION=0.10.18
oc import-image gogs:0.10.18
oc policy add-role-to-group view system:authenticated -n gogs

